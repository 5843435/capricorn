<%- model_class = Stock -%>
<div class="page-header">
  <h3><%=t '.title', :default => [:'helpers.titles.new', 'New %{model}'], :model => model_class.model_name.human.titleize %></h3>
</div>
<% if user_signed_in? %>
<%= link_to t('.new', :default => t("オリジナル商品追加")),
            new_user_item_path,
            :class => 'btn btn-primary' %>
  <%= render :partial => 'form' %>
<% else %>
  <%= render :partial => 'form2' %>
<% end %>


